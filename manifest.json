{
    "name": "Lyric Finder",
    "description": "A tool used to find the lyrics from a music video on YouTube.",
    "version": "1.0", 
    "manifest_version": 2,
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    }, 
    "permissions": [
        "tabs", 
        "https://www.youtube.com/*"
    ]
    
}